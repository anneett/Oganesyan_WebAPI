    "components": {
        "schemas": {
            "Exercise": {
                "required": [
                    "correctAnswer",
                    "difficulty",
                    "title"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "title": {
                        "maxLength": 200,
                        "minLength": 1,
                        "type": "string"
                    },
                    "difficulty": {
                        "$ref": "#/components/schemas/ExerciseDifficulty"
                    },
                    "correctAnswer": {
                        "minLength": 1,
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "ExerciseCreateDto": {
                "required": [
                    "correctAnswer",
                    "difficulty",
                    "title"
                ],
                "type": "object",
                "properties": {
                    "title": {
                        "maxLength": 200,
                        "minLength": 1,
                        "type": "string"
                    },
                    "difficulty": {
                        "$ref": "#/components/schemas/ExerciseDifficulty"
                    },
                    "correctAnswer": {
                        "minLength": 1,
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "ExerciseDifficulty": {
                "enum": [
                    0,
                    1,
                    2
                ],
                "type": "integer",
                "format": "int32"
            },
            "ExerciseStatsDto": {
                "type": "object",
                "properties": {
                    "exerciseId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "exerciseTitle": {
                        "type": "string",
                        "nullable": true
                    },
                    "totalAttempts": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "uniqueUsers": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "correctAnswers": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "percentCorrect": {
                        "type": "number",
                        "format": "double"
                    }
                },
                "additionalProperties": false
            },
            "LoginData": {
                "type": "object",
                "properties": {
                    "login": {
                        "type": "string",
                        "nullable": true
                    },
                    "frontendHash": {
                        "type": "string",
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "RefreshRequest": {
                "required": [
                    "refreshToken"
                ],
                "type": "object",
                "properties": {
                    "refreshToken": {
                        "type": "string",
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "Solution": {
                "required": [
                    "exerciseId",
                    "userAnswer"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "userId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "exerciseId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "exercise": {
                        "$ref": "#/components/schemas/Exercise"
                    },
                    "userAnswer": {
                        "minLength": 1,
                        "type": "string"
                    },
                    "isCorrect": {
                        "type": "boolean"
                    },
                    "submittedAt": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "result": {
                        "type": "string",
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "SolutionCreateDto": {
                "required": [
                    "exerciseId",
                    "userAnswer"
                ],
                "type": "object",
                "properties": {
                    "exerciseId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "userAnswer": {
                        "minLength": 1,
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "UserCreateDto": {
                "required": [
                    "login",
                    "passwordHash",
                    "userName"
                ],
                "type": "object",
                "properties": {
                    "userName": {
                        "maxLength": 50,
                        "minLength": 1,
                        "type": "string"
                    },
                    "login": {
                        "maxLength": 50,
                        "minLength": 1,
                        "type": "string"
                    },
                    "passwordHash": {
                        "minLength": 1,
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "UserDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "userName": {
                        "type": "string",
                        "nullable": true
                    },
                    "login": {
                        "type": "string",
                        "nullable": true
                    },
                    "isAdmin": {
                        "type": "boolean"
                    }
                },
                "additionalProperties": false
            },
            "UserSolutionDto": {
                "required": [
                    "exerciseDifficulty"
                ],
                "type": "object",
                "properties": {
                    "solutionId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "userId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "exerciseId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "exerciseTitle": {
                        "type": "string",
                        "nullable": true
                    },
                    "exerciseDifficulty": {
                        "$ref": "#/components/schemas/ExerciseDifficulty"
                    },
                    "correctAnswer": {
                        "type": "string",
                        "nullable": true
                    },
                    "userAnswer": {
                        "type": "string",
                        "nullable": true
                    },
                    "isCorrect": {
                        "type": "boolean"
                    },
                    "submittedAt": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "result": {
                        "type": "string",
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "UserUpdateDto": {
                "type": "object",
                "properties": {
                    "userName": {
                        "maxLength": 50,
                        "type": "string",
                        "nullable": true
                    },
                    "passwordHash": {
                        "type": "string",
                        "nullable": true
                    }
                },
                "additionalProperties": false
            }
        },
        "securitySchemes": {
            "Bearer": {
                "type": "http",
                "description": "Вставьте JWT токен",
                "scheme": "bearer",
                "bearerFormat": "JWT"
